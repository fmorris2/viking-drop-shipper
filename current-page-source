    window.SS && SS.Require(function () {

        function waitForPageLoad(callback) {
            if (document.readyState == 'complete') {
                console.log('ready state complete');
                return callback();
            } else {
                console.log('else');
                if (document.readyState !== 'complete') {
                    console.log('ready state not complete');
                    return setTimeout(waitForPageLoad.bind(null, callback), 30);
                }
            }
        }

        function addEventListeners() {
            window.clickMetricToggle = false;

            // Forgot password page displayed not react
            SS.SPA.dynamicModify({
                "staticContainer": "body",
                "dynamicContainer": "div.sc-club-details",
                "callbackFunction": function () {

                    let clubElement = document.querySelector('div.sc-club-details');

                    if(clubElement.classList.contains('ssChecked') === false){
                        clubElement.classList.add('ssChecked');
                        var randomValue = Math.floor(Math.random() * 10000) + 1;
                        var xhttp = new XMLHttpRequest();
                        xhttp.open("POST", "/__ssobj/track?event=clubPageViewed&value=undefined&x=" + randomValue, true);
                        xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
                        xhttp.send();
                    }

                },
                "runCallbackNow": false,
                "watchSubtree": true
            });

        };

        waitForPageLoad(function () {
            console.log('wait for page load called');
            addEventListeners();
        });

    });

</script>
<script>
    (function () {

        var sitespect_url = location.pathname + location.search;

        function checkUrl(sitespect_url) {
            var clubPartialRegex = /\/club\/[0-9]+(?:$|\?.*)/gi;
            var clubFullRegex = /\/club\/[^/]+\/[0-9]+(?:$|\?.*)/gi;
            var matchesClubRegex = clubPartialRegex.test(sitespect_url);

            if(matchesClubRegex !== true){
                matchesClubRegex = clubFullRegex.test(sitespect_url);
            }

            if(matchesClubRegex === true){
                var randomValue = Math.floor(Math.random() * 10000) + 1;
                var xhttp = new XMLHttpRequest();
                xhttp.open("POST", "/__ssobj/track?event=clubPageViewedV2&value=undefined&x=" + randomValue, true);
                xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
                xhttp.send();
            }
        }

        var checkIfUrlChanged = function () {
            setTimeout(function () {

                // If the URL has changed
                if (sitespect_url !== location.pathname + location.search) {

                    // Set the new URL as current
                    sitespect_url = location.pathname + location.search;

                    // Print the New URL
                    console.log("New Page: " + sitespect_url);

                    checkUrl(sitespect_url);

                }
            }, 750);
        };

        document.body.addEventListener('click', function () {
            checkIfUrlChanged();
        });

        window.addEventListener('popstate', checkIfUrlChanged);

        checkUrl(sitespect_url);

    }());
</script>
<script>
    window.SS && SS.Require(function () {

        function waitForPageLoad(callback) {
            if (document.readyState == 'complete') {
                console.log('ready state complete');
                return callback();
            } else {
                console.log('else');
                if (document.readyState !== 'complete') {
                    console.log('ready state not complete');
                    return setTimeout(waitForPageLoad.bind(null, callback), 30);
                }
            }
        }

        function addEventListeners() {
            window.clickMetricToggle = false;

            // Login Page Viewed
            SS.SPA.dynamicModify({
                "staticContainer": "body",
                "dynamicContainer": "div.sc-auth-page.sc-login",
                "callbackFunction": function () {
                    
                    let loginPage = document.querySelector('div.sc-auth-page.sc-login');
                    if(loginPage !== null && loginPage !== undefined){
                        if(loginPage.classList.contains('ssChecked') === false) {
                            loginPage.classList.add('ssChecked');
                            SS.EventTrack.rp('loginPageViewed');
                        }  
                    }

                },
                "runCallbackNow": false,
                "watchSubtree": true
            });
            // Forgot password Page Viewed
            SS.SPA.dynamicModify({
                "staticContainer": "body",
                "dynamicContainer": "div.sc-forgot-password-page",
                "callbackFunction": function () {

                    let forgotPasswordPage = document.querySelector('div.sc-forgot-password-page');
                    if(forgotPasswordPage !== null && forgotPasswordPage !== undefined) {
                        if (forgotPasswordPage.classList.contains('ssChecked') === false) {
                            forgotPasswordPage.classList.add('ssChecked');
                            SS.EventTrack.rp('forgotPasswordPageViewed');
                        }
                    }

                },
                "runCallbackNow": false,
                "watchSubtree": true
            });
            // Forgot email Page Viewed
            SS.SPA.dynamicModify({
                "staticContainer": "body",
                "dynamicContainer": "div.sc-forgot-email-page",
                "callbackFunction": function () {

                    let forgotEmailPage = document.querySelector('div.sc-forgot-email-page');
                    if(forgotEmailPage !== null && forgotEmailPage !== undefined) {
                        if (forgotEmailPage.classList.contains('ssChecked') === false) {
                            forgotEmailPage.classList.add('ssChecked');
                            SS.EventTrack.rp('forgotEmailPageViewed');
                        }
                    }

                },
                "runCallbackNow": false,
                "watchSubtree": true
            });

        };

        waitForPageLoad(function () {
            console.log('wait for page load called');
            addEventListeners();
        });

    });

</script>

