                 function createCookie(name,value,days) {
                        var expires = "";
                        if (days) {
                            var date = new Date();
                            date.setTime(date.getTime() + (days*24*60*60*1000));
                            expires = "; expires=" + date.toUTCString();
                        }
                        document.cookie = name + "=" + value + expires + "; path=/";
                    }
    
                    function readCookie(name) {
                        var nameEQ = name + "=";
                        var ca = document.cookie.split(';');
                        for(var i=0;i < ca.length;i++) {
                            var c = ca[i];
                            while (c.charAt(0)==' ') c = c.substring(1,c.length);
                            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
                        }
                        return null;
                    }
                    
                 $(window).load(function(){
                    $('#signInPageErrorDisplay').focus();
                    $('#box').after('<span></span>');
                     
                    if (document.cookie.indexOf('prftStLog') > -1 ) {
                        var currentBox = readCookie('prftStLog');
                        $('#box').val(currentBox);
                        if (currentBox === 'true'){
                            $('#box').attr('checked', true);
                        } else {
                            $('#box').attr('checked', false);
                        }
                    } else {
                        createCookie('prftStLog', 'true', 365);
                    }
                    $('#box').change(function() {
                        var setBox = $('#box').val();
                        if (setBox === 'true'){
                            $(this).val('false').attr('checked', false);
                            createCookie('prftStLog', 'false', 365);
                        } else {
                            $(this).val('true').attr('checked', true);
                            createCookie('prftStLog', 'true', 365);
                        }
                    });
                 });
             </script> <!--  Membership registration section starts --> <!--  Membership registration section starts --> <div class="registerPanel span6 offset0"> <h2 class="drawerH1">Register<span class="sml4shelf"> your Club membership and create an online account</span></h2> <form id="regform" method="post" novalidate="novalidate" action="https://www.samsclub.com:443/sams/account/registration/registration.jsp?src=draw"> <fieldset class="top-20"> <legend class="sr-text">To Register, please enter your Membership number and ZIP Code</legend> <div class="formFieldElement"> <label class="lblWMLTxt" for="txtMemID1">Membership number*</label> <input type="text" id="txtMemID1" name="memId" maxlength="17" class="membership span5 offset0" autocomplete="off" required="required"> <div class="membershipHelpIcn drwSprite dwr007_help" tabindex="0" role="tooltip" aria-label="where is my membership number?" aria-describedby="regttip rtip1 rtip2"> <div class="ttWrapper memSgntt"> <span id="regttip" class="renew_help">Please find the membership number on the back of your membership card.</span> <img aria-hidden="true" class="renew_card" src="/sams/assets/membership/membership_help_image.png" alt="" width="100" height="61"> </div> </div> </div> <div class="formFieldElement"> <label class="lblWMLTxt" for="txtMemZip1">ZIP Code*</label> <input type="text" id="txtMemZip1" name="zipCode" class="zipcode span5 offset0" autocomplete="off" required="required"> </div> <div class="formFieldElement hideit"> <label for="phone1" class="lblWMLTxt">Phone number*</label><input id="phone1" name="phone" maxlength="16" class="telephone span5" type="text" value=""></div> <p class="zipmsg1">Your ZIP Code must match your current account information.</p> <p class="phonemsg1 hideit">Your Phone Number must match your current account information.</p> <button type="button" class="choosephone1">Use your phone number instead.</button> <button type="button" class="tryzipc1 hideit">Use your ZIP Code instead.</butt